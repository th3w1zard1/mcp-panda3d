{
  "name": "@th3w1zard1/mcppanda3d",
  "version": "0.1.3",
  "description": "panda3d mcp tools and utilities for Claude and other LLM AI models",
  "private": false,
  "type": "module",
  "main": "./build/index.js",
  "bin": "./build/index.js",
  "files": [
    "build"
  ],
  "scripts": {
    "build": "npm install && tsc && node -e \"require('fs').chmodSync('build/index.js', '755')\"",
    "check-format": "prettier --check \"src/**/*.ts\"",
    "format": "prettier --write \"src/**/*.ts\"",
    "inspector": "npx @modelcontextprotocol/inspector build/index.js",
    "lint:fix": "eslint src/ --fix",
    "lint": "eslint src/",
    "prepare": "npm run build",
    "release": "npm run build && npm publish --access public",
    "update": "npm install && npm run build",
    "watch": "tsc --watch"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "0.6.0",
    "axios": "^1.8.4",
    "cheerio": "^1.0.0",
    "puppeteer": "^23.11.1"
  },
  "devDependencies": {
    "@types/node": "^20.17.30",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "prettier": "^3.5.3",
    "typescript": "^5.8.2",
    "typescript-eslint": "^7.18.0"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/th3w1zard1/mcp-panda3d.git"
  },
  "keywords": [
    "panda3d",
    "mcp",
    "3d",
    "design",
    "docs"
  ],
  "author": "th3w1zard1",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/th3w1zard1/mcp-panda3d/issues"
  },
  "homepage": "https://github.com/th3w1zard1/mcp-panda3d#readme"
}
